                                    <path fill-rule="evenodd"
                                        d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4h-.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1v1zM4.118 4L4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4H4.118zM2.5 3V2h11v1h-11z" />
                                </svg></button>

                        </div>


                    </div>
                    <div class="card-body">
                        <div class="form-row">


                            <div class="col-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-text">Type</div>
                                    <select>
                                        <optgroup label="HTTP Proxy">
                                            <option value="http">http</option>
                                            <option value="https">https</option>
                                        </optgroup>
                                        <optgroup label="TCP-IP Proxy">
                                            <option value="tcpip">tcpip</option>
                                        </optgroup>
                                    </select>
                                </div>
                            </div>

                            <div class="col-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-text">Port</div>
                                    <input type="text" class="form-control" placeholder="8080">
                                </div>
                            </div>

                            <div class="col">
                                <div class="input-group-prepend">
                                    <div class="input-group-text">Destination</div>
                                    <input type="text" class="form-control" placeholder="http://">
                                </div>
                            </div>

                            <div class="col">
                                <button type="button" class="btn btn-secondary">Activate</button>
                            </div>


                        </div>
                    </div>
                </div>


<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <title>HTTPMon</title>
    <meta http-equiv="Content-Security-Policy" content="script-src 'self' 'unsafe-inline';" />


    <link rel="stylesheet" href="/bootstrap/css/bootstrap.min.css" />
    <script>if (typeof module === 'object') { window.module = module; module = undefined; }</script>
    <script src="/jquery-3.5.1.min.js"></script>
    <script src="/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script>if (window.module) module = window.module;</script>

    <link href="/index.css" rel="stylesheet">
</head>

<body style="background: white">

    <div class="d-flex" id="wrapper">

        <!-- Sidebar -->
        <div class="bg-dark border-right" id="sidebar-wrapper">
            <div class="sidebar-heading">Monitors</div>
            <div class="list-group" id="monitorlist">

                <% monitors.forEach(function (monitor) { %>
                <% 
                    // set active class on selected monitor
                    var activeClass = ''
                    if (currentMonitor === monitor.id) { activeClass = ' active ' };
                %>
                <a href="/views/monitor/<%=monitor.id%>/" class="list-group-item list-group-item-action list-group-item-dark <%=activeClass%>"
                    aria-label="Monitor (<%= monitor.name %>)">
                    <div class="d-flex w-100 justify-content-between align-items-center">
                        <h5 class="mb-1"><%= monitor.name %></h5>
                        <span class="badge badge-dark">14</span>
                    </div>
                    <p class="mb-1">local: tcp://*:<%=monitor.localPort%></p>
                    <small>tcp://<%=monitor.remoteHost%>:<%=monitor.remotePort%></small>
                </a>
                <% }) %>

                <a href="#" class="list-group-item list-group-item-action list-group-item-dark"
                    aria-label="Monitor (127.0.0.1:8080)">
                    <div class="d-flex w-100 justify-content-between align-items-center">
                        <h5 class="mb-1">127.0.0.1:8080</h5>
                        <small class="text-muted">Inaktive</small>
                    </div>
                    <p class="mb-1">src(HTTP): 9090</p>
                    <small>http://www.google.com:8080</small>
                </a>
                <a href="#" class="list-group-item list-group-item-action list-group-item-dark active"
                    aria-label="Monitor (google.com)">
                    <div class="d-flex w-100 justify-content-between align-items-center">
                        <h5 class="mb-1">google.com</h5>
                        <span class="badge badge-dark">1</span>
                    </div>
                    <p class="mb-1">src(HTTP): 9090</p>
                    <small>http://www.google.com:8080</small>
                </a>

                <a href="#" class="list-group-item list-group-item-action list-group-item-dark">
                    <div class="d-flex w-100 justify-content-center align-items-center">
                        <button type="button" class="btn btn-secondary" aria-label="Add new monitor"><svg
                                xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
                                class="bi bi-plus" viewBox="0 0 16 16">
                                <path
                                    d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4z" />
                            </svg></button>
                    </div>
                </a>
            </div>
        </div>
        <!-- /#sidebar-wrapper -->

